<h2>Users</h2>
<table>
    <thead>
        <tr>
            <th>image</th>
            <th>name</th>
        </tr>
    </thead>
    
    <% @users.each do |user| %>
    <tbody>
        <tr>
            <td><%= image_tag user.get_image %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
    </tbody>
    <% end %>
</table>

<%= render 'books/list', book: @book, user: @user %>
